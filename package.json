{
	"name": "bento",
	"version": "1.0.0",
	"description": "Discord Bot with server moderation tools and various entertaining commands. Discord.js v13 rewrite of bentoTS",
	"engines": {
		"node": ">=16.6.0"
	},
	"type": "module",
	"exports": [
		"./dist/start.js",
		"./dist/app.js"
	],
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"rebuild": "yarn clean && yarn build",
		"clean": "git clean -xdf --exclude=\"/config/**/*\"",
		"format": "prettier --check .",
		"format:fix": "prettier --write .",
		"build": "tsc --project tsconfig.json",
		"commands:register": "npm run build && node --enable-source-maps dist/start.js --register",
		"commands:clear": "npm run build && node --enable-source-maps dist/start.js --clear",
		"start": "npm run start:bot",
		"start:bot": "npm run build && node --enable-source-maps dist/start.js",
		"start:manager": "npm run build && node --enable-source-maps dist/manager.js",
		"lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
		"lint:fix": "npx eslint . --fix --ext .js,.jsx,.ts,.tsx",
		"db": "npx prisma db pull && npx prisma generate",
		"clean:dry": "git clean -xdf --exclude=\"/config/**/*\" --dry-run"
	},
	"keywords": [],
	"author": "banner4422",
	"license": "GPL-3.0",
	"devDependencies": {
		"@types/express": "^4.17.13",
		"@types/luxon": "^2.3.1",
		"@types/node": "^17.0.23",
		"@types/node-fetch": "^3.0.3",
		"@types/node-schedule": "^1.3.2",
		"@types/remove-markdown": "^0.3.1",
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@typescript-eslint/parser": "^5.25.0",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"prettier": "^2.6.1",
		"prisma": "^3.11.1",
		"ts-node": "^10.7.0",
		"ts-node-dev": "^1.1.8",
		"typescript": "^4.6.3"
	},
	"dependencies": {
		"@discordjs/rest": "^0.3.0",
		"@prisma/client": "^3.11.1",
		"@top-gg/sdk": "^3.1.3",
		"axios": "^0.26.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"discord.js": "^13.6.0",
		"discord.js-rate-limiter": "^1.3.2",
		"dotenv": "^16.0.0",
		"express": "^4.17.3",
		"express-promise-router": "^4.1.1",
		"filesize": "^8.0.7",
		"luxon": "^2.3.1",
		"node-fetch": "^3.2.3",
		"node-schedule": "^2.1.0",
		"node-vibrant": "^3.2.1-alpha.1",
		"pino": "^7.9.2",
		"pino-pretty": "^7.6.0",
		"remove-markdown": "^0.3.0"
	},
	"prisma": {
		"schema": "prisma/schema.prisma"
	}
}
