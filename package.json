{
	"name": "bento",
	"version": "1.0.0",
	"description": "Discord Bot with server moderation tools and various entertaining commands. Discord.js v13 rewrite of bentoTS",
	"engines": {
		"node": ">=16.6.0"
	},
	"type": "module",
	"exports": [
		"./dist/start.js",
		"./dist/app.js"
	],
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"rebuild": "yarn clean && yarn build",
		"clean": "git clean -xdf --exclude=\"/config/**/*\"",
		"format": "prettier --check .",
		"format:fix": "prettier --write .",
		"build": "tsc --project tsconfig.json",
		"commands:view": "npm run build && node --enable-source-maps dist/start.js commands view",
		"commands:register": "npm run build && node --enable-source-maps dist/start.js commands register",
		"commands:rename": "npm run build && node --enable-source-maps dist/start.js commands rename",
		"commands:delete": "npm run build && node --enable-source-maps dist/start.js commands delete",
		"commands:clear": "npm run build && node --enable-source-maps dist/start.js commands clear",
		"start": "npm run start:bot",
		"start:bot": "npm run build && node --enable-source-maps dist/start.js",
		"start:manager": "npm run build && node --enable-source-maps dist/manager.js",
		"lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
		"lint:fix": "npx eslint . --fix --ext .js,.jsx,.ts,.tsx",
		"db": "npx prisma db pull && npx prisma generate",
		"clean:dry": "git clean -xdf --exclude=\"/config/**/*\" --dry-run"
	},
	"keywords": [],
	"author": "banner4422",
	"license": "GPL-3.0",
	"devDependencies": {
		"@types/chroma-js": "^2.1.4",
		"@types/express": "^4.17.16",
		"@types/luxon": "^3.2.0",
		"@types/node": "^18.11.18",
		"@types/node-fetch": "^3.0.3",
		"@types/node-schedule": "^2.1.0",
		"@types/remove-markdown": "^0.3.1",
		"@types/spotify-web-api-node": "^5.0.7",
		"@types/tz-lookup": "^6.1.0",
		"@types/utf8": "^3.0.1",
		"@typescript-eslint/eslint-plugin": "^5.49.0",
		"@typescript-eslint/parser": "^5.49.0",
		"eslint": "^8.33.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-prettier": "^4.2.1",
		"prettier": "^2.8.3",
		"prisma": "^4.9.0",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"typescript": "^4.9.4"
	},
	"dependencies": {
		"@discordjs/rest": "^1.5.0",
		"@prisma/client": "^4.9.0",
		"@top-gg/sdk": "^3.1.3",
		"axios": "^1.2.6",
		"aztro-js": "^0.1.5",
		"chroma-js": "^2.4.2",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"country-emoji": "^1.5.6",
		"discord-api-types": "^0.37.30",
		"discord.js": "^14.7.1",
		"discord.js-rate-limiter": "^1.3.2",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"express-promise-router": "^4.1.1",
		"filesize": "^10.0.6",
		"luxon": "^3.2.1",
		"naughty-words": "^1.2.0",
		"node-fetch": "^3.3.0",
		"node-schedule": "^2.1.1",
		"node-vibrant": "^3.1.6",
		"pino": "^8.8.0",
		"pino-pretty": "^9.1.1",
		"remove-markdown": "^0.5.0",
		"spotify-web-api-node": "^5.0.2",
		"tz-lookup": "^6.1.25",
		"utf8": "^3.0.0"
	},
	"prisma": {
		"schema": "prisma/schema.prisma"
	}
}
