{
	"name": "bento2",
	"version": "1.0.0",
	"description": "Discord Bot with server moderation tools and various entertaining commands. Discord.js v13 rewrite of bentoTS",
	"engines": {
		"node": ">=16.6.0"
	},
	"main": "dist/start.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"rebuild": "yarn clean && yarn build",
		"clean": "rm -rf dist/",
		"format": "prettier --check .",
		"format:fix": "prettier --write .",
		"build": "tsc --project tsconfig.json",
		"register": "npm run build && node --enable-source-maps dist/start.js --register",
		"start": "npm run build && node --enable-source-maps dist/start.js",
		"start:dev": "ts-node-dev --watch src/**/*.ts,config/**/*.json,lang/**/*.json src/start.ts",
		"start:shard": "npm run build && node --enable-source-maps dist/app.js",
		"lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
		"lint:fix": "npx eslint . --fix --ext .js,.jsx,.ts,.tsx"
	},
	"keywords": [],
	"author": "banner4422",
	"license": "GPL-3.0",
	"devDependencies": {
		"@types/express": "^4.17.13",
		"@types/node": "^17.0.8",
		"@types/node-fetch": "^3.0.3",
		"@types/node-schedule": "^1.3.2",
		"@types/remove-markdown": "^0.3.1",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"eslint": "^8.6.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"prettier": "^2.5.1",
		"prisma": "^3.7.0",
		"ts-node": "^10.4.0",
		"ts-node-dev": "^1.1.8",
		"typescript": "^4.5.4"
	},
	"dependencies": {
		"@discordjs/rest": "^0.2.0-canary.0",
		"@prisma/client": "^3.7.0",
		"axios": "^0.24.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"discord.js": "^13.5.1",
		"discord.js-rate-limiter": "^1.3.1",
		"express": "^4.17.2",
		"express-promise-router": "^4.1.1",
		"filesize": "^8.0.6",
		"node-schedule": "^2.1.0",
		"pino": "^7.6.2",
		"pino-pretty": "^7.3.0",
		"remove-markdown": "^0.3.0"
	}
}
